    <DataSet Name="Defaults">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <CommandText>WITH
current_year
AS
(
	SELECT 
		  [year_first_date] = DATEADD(yy, DATEDIFF(yy, 0, GETDATE()), 0)
)
, 
year_end_date
AS
(
	SELECT
		  [year_first_date]
		, [year_last_date]  =  DATEADD(yy, DATEDIFF(yy, 0, [year_first_date]) + 1, -1)
	FROM 
		current_year
)
, 
default_values
AS
(
	SELECT
		  [year_first_date]
		, [year_last_date]
		, [year_first_monday] = DATEADD(wk, DATEDIFF(wk,0, [year_first_date]), 0)
		, [year_last_monday] = DATEADD(wk, DATEDIFF(wk,0, [year_last_date]), 0)
	FROM 
		year_end_date
)
SELECT
	  [year_date] = YEAR([year_first_date])
	, [year_first_date]
	, [year_last_date]
	, [year_first_monday] = 
		CASE 
			WHEN YEAR([year_first_date]) != YEAR([year_first_monday]) THEN DATEADD(d, 7, [year_first_monday])
			ELSE [year_first_monday]
		END 
	, [year_last_monday] = 
		CASE 
			WHEN YEAR([year_first_date]) != YEAR([year_last_monday]) THEN DATEADD(d, -7, [year_last_monday])
			ELSE [year_last_monday]
		END 
	, [current_week_monday] = DATEADD(wk, DATEDIFF(wk,0, GETDATE()), 0)
	, [null_string] = '&lt;Undefined&gt;'
	, [all_value] = '&lt;ALL&gt;'
	, [no_selection] = '&lt;NONE&gt;'
FROM 
	default_values</CommandText>
      </Query>
      <Fields>
        <Field Name="year_date">
          <DataField>year_date</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="year_first_date">
          <DataField>year_first_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="year_last_date">
          <DataField>year_last_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="year_first_monday">
          <DataField>year_first_monday</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="year_last_monday">
          <DataField>year_last_monday</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="current_week_monday">
          <DataField>current_week_monday</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="null_string">
          <DataField>null_string</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="all_value">
          <DataField>all_value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="no_selection">
          <DataField>no_selection</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
