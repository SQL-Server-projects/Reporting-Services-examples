    <DataSet Name="ReportFolder">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@all_value">
            <Value>=Parameters!all_value.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH 
catalog_type_description
AS
(
	SELECT tbl.* FROM (VALUES
	  ( 1, 'Folder')
	, ( 2, 'Report')
	, ( 3, 'Resource')
	, ( 4, 'Linked Report')
	, ( 5, 'Data Source')
	, ( 6, 'Report Model')
	, ( 8, 'Shared Dataset')
	, ( 9, 'Report Part')
	) tbl ([TypeID], [TypeDescription]) 
	WHERE [TypeID] = 2
)
SELECT 
	[Report_Folder] = SUBSTRING(c.[Path], 2, Len(c.[Path])-Len(c.[Name])-2)   
FROM 
	ReportServer.dbo.[Catalog] AS c WITH (NOLOCK) 
	INNER JOIN catalog_type_description AS ctd ON ctd.[TypeID] = c.[Type]
UNION 
SELECT @all_value
ORDER BY 1</CommandText>
      </Query>
      <Fields>
        <Field Name="Report_Folder">
          <DataField>Report_Folder</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ReportName">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@all_value">
            <Value>=Parameters!all_value.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportFolder">
            <Value>=Parameters!ReportFolder.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
DECLARE @Report_Folder AS VARCHAR(450)
SET @Report_Folder = '&lt;ALL&gt;'
*/

SELECT DISTINCT
	  ReportName = c.Name
	, ReportFolder = SUBSTRING(c.Path, 2, Len(c.Path)-Len(c.Name)-2)  
FROM 
	ReportServer.dbo.ExecutionLog el 
	INNER JOIN ReportServer.dbo.Catalog c ON el.ReportID = c.ItemID
WHERE 
	c.[TYPE] = 2
	AND (@all_value IN (@ReportFolder) OR SUBSTRING(c.Path, 2, Len(c.Path)-Len(c.Name)-2) IN(@ReportFolder))
UNION 
SELECT @all_value, @all_value
ORDER BY 1</CommandText>
      </Query>
      <Fields>
        <Field Name="ReportName">
          <DataField>ReportName</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="ReportFolder">
          <DataField>ReportFolder</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="UserNames">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@all_value">
            <Value>=Parameters!all_value.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportFolder">
            <Value>=Parameters!ReportFolder.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportName">
            <Value>=Parameters!ReportName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT	
	[UserName] = RIGHT(el.[UserName],(LEN(el.[UserName])-CHARINDEX('\',el.[UserName])))  
FROM 
	ReportServer.dbo.[ExecutionLog] el 
	INNER JOIN ReportServer.dbo.[Catalog] c ON el.[ReportID] = c.[ItemID]
WHERE 
	c.[TYPE] = 2
	AND (@all_value IN (@ReportFolder) OR SUBSTRING(c.[Path], 2, Len(c.[Path])-Len(c.[Name])-2) IN(@ReportFolder))
	AND (@all_value IN(@ReportName) OR c.Name  IN(@ReportName))
UNION SELECT @all_value</CommandText>
      </Query>
      <Fields>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ReportStatus">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@all_value">
            <Value>=Parameters!all_value.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT	
el.[Status]
FROM ReportServer.dbo.[ExecutionLog] el 
UNION SELECT @all_value</CommandText>
      </Query>
      <Fields>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
