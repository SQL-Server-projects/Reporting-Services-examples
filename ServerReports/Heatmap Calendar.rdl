<?xml version="1.0" encoding="utf-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <Description>Execution Log Heatmap Report</Description>
  <Author>Anthony Duguid</Author>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="ReportServer">
      <DataSourceReference>ReportServer</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>9c5d5e18-ddee-41b3-ae49-8d83f261caf6</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Calendar">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@all_value">
            <Value>=Parameters!all_value.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LogStatus">
            <Value>=Parameters!LogStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportFolder">
            <Value>=Parameters!ReportFolder.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportName">
            <Value>=Parameters!ReportName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@day_of_week">
            <Value>=Parameters!day_of_week.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*'---------------------------------------------------------------------------------------
' Purpose:	to search the reporting services execution log
'-----------------------------------------------------------------------------------------

DECLARE @all_value AS VARCHAR(10)
DECLARE @LogStatus AS VARCHAR(50)
DECLARE @ReportFolder AS VARCHAR(450)
DECLARE @ReportName AS VARCHAR(450)
DECLARE @UserName AS VARCHAR(260)
DECLARE @GroupByColumn AS VARCHAR(50)
DECLARE @StartDate AS DATETIME
DECLARE @EndDate AS DATETIME

SET @all_value = '&lt;ALL&gt;'
SET @LogStatus = '&lt;ALL&gt;'
SET @ReportFolder = '&lt;ALL&gt;'
SET @ReportName = '&lt;ALL&gt;' 
SET @UserName = '&lt;ALL&gt;'
SET @GroupByColumn = 'Report Folder'
SET @StartDate = NULL
SET @EndDate = NULL

*/


;WITH 
week_list
AS
(
	SELECT 
		  [week_date] = DATEADD(d, 1, DATEADD(wk, wks.[number], DATEADD(yy, DATEDIFF(yy, 0, @StartDate), 0)))
	FROM 
		master.dbo.[spt_values] AS wks
	WHERE 
		1=1 -- used in testing to be able to comment out other clauses below
		AND wks.[type] = 'P'
		AND wks.[number] &lt;= 255
)
, 
week_list_format
AS
(
	SELECT
		  [front_end_week_number] = DATEPART(wk, yw.[week_date])
		, yw.[week_date]
	FROM
		week_list AS yw
	WHERE 
		1=1 -- used in testing to be able to comment out other clauses below
		AND (yw.[week_date] BETWEEN @StartDate AND @EndDate)
)
, 
day_list
AS
(
	SELECT tbl.* FROM (VALUES
	  ( 0, 'Monday')
	, ( 1, 'Tuesday')
	, ( 2, 'Wednesday')
	, ( 3, 'Thursday')
	, ( 4, 'Friday')
	, ( 5, 'Saturday')
	, ( 6, 'Sunday')
	) tbl ([day_of_week], [day_name]) 
)
,
report_users 
AS
(
	SELECT [UserID], [UserName], [SimpleUserName] = UPPER(RIGHT([UserName], (LEN([UserName])-CHARINDEX('\', [UserName])))) FROM ReportServer.dbo.[Users]
)
,
report_catalog
AS
(
	SELECT    
		  rpt.[ItemID]
		, rpt.[CreatedById]
		, rpt.[ModifiedById]
		, rpt.[Type]
		, rpt.[Name] 
		, [ReportName] = rpt.[Name] 
		, rpt.[Description]
		, rpt.[Parameter]
		, [CreationDate] = CONVERT(DATETIME, CONVERT(VARCHAR(11), rpt.[CreationDate], 13))
		, [ModifiedDate] = CONVERT(DATETIME, CONVERT(VARCHAR(11), rpt.[ModifiedDate], 13))
		, [ReportFolder] = SUBSTRING(rpt.[Path], 2, Len(rpt.[Path])-Len(rpt.[Name])-2) 
		, rpt.[Path]
		, [URL_ReportFolder] = 'http://' + Host_Name() + '/Reports/Pages/Report.aspx?ItemPath=%2f'  + SUBSTRING(rpt.[Path], 2, Len(rpt.[Path])-Len(rpt.[Name])-2)  + '&amp;ViewMode=List'
		, [URL_Report] = 'http://' + Host_Name() + '/Reports/Pages/Report.aspx?ItemPath=%2f'  + SUBSTRING(rpt.[Path], 2, Len(rpt.[Path])-Len(rpt.[Name])-2)  + '%2f' + rpt.[Name]
		--, [ReportDefinition] = CONVERT(VARCHAR(MAX), CONVERT(VARBINARY(MAX), rpt.Content))  
		, [HostName] = Host_Name()
	FROM 
		ReportServer.dbo.[Catalog] AS rpt
	WHERE 
		1=1
		AND rpt.[Type] = 2
)
, 
execution_log
AS
(
SELECT 
	  ReportName = rpt.[Name] 
	, day_name = DATENAME(dw, CONVERT(DATE, el.TimeStart))
	, day_of_week = DATEPART(dw, CONVERT(DATE, el.TimeStart))
	, front_end_week_number = DATEPART( wk, CONVERT(DATE, el.TimeStart))
	, week_date = DATEADD(dd, -(DATEPART(dw, CONVERT(DATE, el.TimeStart))-1), CONVERT(DATE, el.TimeStart)) 
	, day_event_count = COUNT(rpt.[Name]) OVER (PARTITION BY CONVERT(DATE, el.TimeStart) ORDER BY CONVERT(DATE, el.TimeStart)) 
FROM 
	report_catalog AS rpt 
	LEFT JOIN ReportServer.dbo.ExecutionLog AS el ON el.ReportID = rpt.ItemID
	LEFT JOIN report_users AS usr ON el.UserName = usr.UserName
WHERE 
	1=1
	AND (@all_value IN(@LogStatus) OR el.[Status] IN(@LogStatus))
	AND (@all_value IN (@ReportFolder) OR rpt.[ReportFolder] IN(@ReportFolder))
	AND (@all_value IN(@ReportName) OR rpt.[ReportName] IN(@ReportName))
	AND (@all_value IN(@UserName) OR usr.[SimpleUserName] IN(@UserName))
	AND (@StartDate IS NULL OR CONVERT(DATETIME, CONVERT(VARCHAR(11), el.[TimeStart], 13)) &gt;= @StartDate)
	AND (@EndDate IS NULL OR CONVERT(DATETIME, CONVERT(VARCHAR(11), el.[TimeStart], 13)) &lt;= @EndDate)
)
SELECT 
	  el.[ReportName]
	, el.[day_name]
	, el.[day_of_week]
	, el.[front_end_week_number]
	, el.[week_date]
	, el.[day_event_count]
FROM 
	execution_log AS el
UNION
SELECT 
	  [ReportName] = 'Week Date'
	, [day_name] = NULL
	, [day_of_week] = NULL
	, wlf.[front_end_week_number]
	, wlf.[week_date]
	, [day_event_count] = 0
FROM 
	  week_list_format AS wlf
	  , execution_log
UNION
SELECT 
	  [ReportName] = 'Day Name'
	, dl.[day_name]
	, dl.[day_of_week]
	, [front_end_week_number] = NULL
	, [week_date] = NULL
	, [day_event_count] = 0
FROM 
	  day_list AS dl
	  , execution_log
WHERE 
	1=1 -- change to 1=2 to hide the filler records during testing
	AND (dl.[day_of_week] IN (@day_of_week)) </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ReportName">
          <DataField>ReportName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="day_name">
          <DataField>day_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="day_of_week">
          <DataField>day_of_week</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="front_end_week_number">
          <DataField>front_end_week_number</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="week_date">
          <DataField>week_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="day_event_count">
          <DataField>day_event_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
