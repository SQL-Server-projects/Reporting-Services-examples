  <DataSets>
    <DataSet Name="FolderPermissions">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@all_value">
            <Value>=Parameters!all_value.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportFolder">
            <Value>=Parameters!ReportFolder.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
|--------------------------------------------------------------------------------------------------
| Purpose:		To show all the Reporting Services folder permissions
| References:	How to create new permission groups https://www.mssqltips.com/sqlservertip/2793/sql-server-reporting-services-2012-permissions/
|				How to query folder permissions http://stackoverflow.com/questions/16920251/sql-server-reporting-services-2008-r2-folder-and-report-security
| Notes:		To deploy this report to the main report page, you must use "Upload File"
|				SQLCmdMode Script
|--------------------------------------------------------------------------------------------------

DECLARE @all_value AS VARCHAR(50)
DECLARE @UserName AS VARCHAR(50)
DECLARE @ReportFolder AS VARCHAR(50)

SET @all_value = '&lt;ALL&gt;'
SET @UserName = '&lt;ALL&gt;'
SET @ReportFolder = '&lt;ALL&gt;'

*/


;WITH 
ad_users ([sam_account_name], [distinguished_name], [surname], [given_name], [display_name], [full_name], [email], [telephone], [extension], [office_location], [department]) --Cannot resolve the collation conflict between "Latin1_General_100_CI_AS_KS_WS" and "Latin1_General_CI_AS" in the equal to operation.
AS 
(
	SELECT 
		  [sam_account_name] = UPPER(adu.[sam_account_name]) Collate Latin1_General_CI_AS
		, adu.[distinguished_name] Collate Latin1_General_CI_AS
		, adu.[surname] Collate Latin1_General_CI_AS
		, adu.[given_name] Collate Latin1_General_CI_AS
		, adu.[display_name] Collate Latin1_General_CI_AS
		, adu.[full_name] Collate Latin1_General_CI_AS
		, adu.[email] Collate Latin1_General_CI_AS
		, adu.[telephone] Collate Latin1_General_CI_AS
		, adu.[extension] Collate Latin1_General_CI_AS
		, adu.[office_location] Collate Latin1_General_CI_AS
		, adu.[department] Collate Latin1_General_CI_AS
	FROM 
		[ACTIVE_DIRECTORY].[ad].[ad_staff] adu
)
,
role_names
AS
(
	SELECT tbl.* FROM (VALUES
	  ( 'Browser')
	, ( 'Content Manager')
	, ( 'Manage Subscriptions')
	, ( 'My Reports')
	, ( 'NavigateFolder')
	, ( 'Publisher')
	, ( 'Report Builder')
	) tbl ([RoleName]) 
)
,
catalog_type_description
AS
(
	SELECT tbl.* FROM (VALUES
	  ( 1, 'Folder')
	, ( 2, 'Report')
	, ( 3, 'Resource')
	, ( 4, 'Linked Report')
	, ( 5, 'Data Source')
	, ( 6, 'Report Model')
	, ( 8, 'Shared Dataset')
	, ( 9, 'Report Part')
	) tbl ([TypeID], [TypeDescription]) 
	WHERE TypeID = 1
)
, 
nonreport_folders
AS
(
	SELECT tbl.* FROM (VALUES
	  ( 'Images')
	, ( 'contents')
	, ( '')
	) tbl ([FolderName]) 
)
, 
user_list
AS
(
	SELECT
		  usr.[UserID]
		, usr.[UserName]
		, [UserNameFormat] = 
			CASE 
				WHEN CHARINDEX('\', usr.[UserName]) &gt; 0 THEN UPPER(SUBSTRING(usr.[UserName] ,CHARINDEX('\', usr.[UserName]) + 1, LEN(usr.[UserName])))
				ELSE usr.[UserName]
			END 
	FROM 
		ReportServer.dbo.[Users] usr
)
SELECT 
	  [FolderName] = cat.[Name]
	, [FolderPath] = cat.[Path]
	, rol.[RoleName]
	, [UserNameFormat] = LOWER(usr.[UserNameFormat])
	, usr.[UserName]
	, [UserNameUrl] = 'http://' + Host_Name() + '/Reports/manage/catalogitem/editrole/' + cat.[Path] + '?user=' + REPLACE(usr.[UserName], N'\', N'%5C') -- must escape the special character \
	, [FolderUrl] = 'http://' + Host_Name() + '/Reports/manage/catalogitem/security' + cat.[Path]
	, adu.[full_name]
	, adu.[email]
FROM 
	ReportServer.dbo.[Catalog] cat
	INNER JOIN catalog_type_description tpd ON cat.[Type] = tpd.[TypeID]	
	LEFT JOIN ReportServer.dbo.[PolicyUserRole] urol ON urol.[PolicyID] = cat.[PolicyID]
	LEFT JOIN ReportServer.dbo.[Roles] rol ON urol.[RoleID] = rol.[RoleID]
	LEFT JOIN ReportServer.dbo.[Policies] pol ON urol.[PolicyID] = pol.[PolicyID]
	LEFT JOIN user_list usr ON urol.[UserID] = usr.[UserID]
	LEFT JOIN nonreport_folders nrf ON nrf.[FolderName] = cat.[Name]
	LEFT JOIN ad_users adu ON adu.[sam_account_name] = usr.[UserNameFormat]
WHERE 
	1=1
	AND nrf.[FolderName] IS NULL
	AND (@all_value IN(@UserName) OR usr.[UserNameFormat] IN(@UserName))
	AND (@all_value IN (@ReportFolder) OR cat.[Path] IN(@ReportFolder))
	AND cat.[Name] NOT IN('System Resources', 'UniversalBrand', 'fbac82c8-9bad-4dba-929f-c04e7ca4111f')
UNION
SELECT
	  [FolderName] = NULL
	, [FolderPath] = NULL
	, rol.[RoleName]
	, [UserNameFormat] = NULL
	, [UserName] = NULL
	, [UserNameUrl] = NULL
	, [FolderUrl] = NULL
	, [full_name] = NULL
	, [email] = NULL
FROM 
	role_names rol</CommandText>
      </Query>
      <Fields>
        <Field Name="FolderName">
          <DataField>FolderName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FolderPath">
          <DataField>FolderPath</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RoleName">
          <DataField>RoleName</DataField>
