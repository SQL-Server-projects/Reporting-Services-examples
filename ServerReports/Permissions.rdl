    <DataSet Name="Users">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@all_value">
            <Value>=Parameters!all_value.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH 
ad_users ([sam_account_name], [full_name]) --Cannot resolve the collation conflict between "Latin1_General_100_CI_AS_KS_WS" and "Latin1_General_CI_AS" in the equal to operation.
AS 
(
	SELECT 
		  [sam_account_name] = UPPER(adu.[sam_account_name]) Collate Latin1_General_CI_AS
		, adu.[full_name] Collate Latin1_General_CI_AS
	FROM 
		[ACTIVE_DIRECTORY].[ad].[ad_staff] adu
)
, 
report_users
AS
(
	SELECT DISTINCT
		[UserNameFormat] = 
			CASE 
				WHEN CHARINDEX('\', usr.[UserName]) &gt; 0 THEN UPPER(SUBSTRING(usr.[UserName] ,CHARINDEX('\', usr.[UserName]) + 1, LEN(usr.[UserName])))
				ELSE usr.[UserName] 
			END 
		, usr.[UserID]
	FROM 
		ReportServer.dbo.[Catalog] cat
		LEFT JOIN ReportServer.dbo.PolicyUserRole urol ON urol.[PolicyID] = cat.[PolicyID]
		LEFT JOIN ReportServer.dbo.Roles rol ON urol.[RoleID] = rol.[RoleID]
		LEFT JOIN ReportServer.dbo.Policies pol ON urol.[PolicyID] = pol.[PolicyID]
		LEFT JOIN ReportServer.dbo.[Users] usr ON urol.[UserID] = usr.[UserID]	
)
SELECT 
	  [UserName] = COALESCE(adu.[full_name], '') + ' (' + LOWER(usr.[UserNameFormat]) + ')'
	, [UserNameFormat]
	, [OrderNumber] = 2
FROM 
	report_users usr
	LEFT JOIN ad_users adu ON adu.[sam_account_name] = usr.[UserNameFormat]
UNION
SELECT @all_value, @all_value, 1
ORDER BY 3, 1</CommandText>
      </Query>
      <Fields>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="UserNameFormat">
          <DataField>UserNameFormat</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="OrderNumber">
          <DataField>OrderNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ReportFolders">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@all_value">
            <Value>=Parameters!all_value.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH 
catalog_type_description
AS
(
	SELECT tbl.* FROM (VALUES
	  ( 1, 'Folder')
	, ( 2, 'Report')
	, ( 3, 'Resource')
	, ( 4, 'Linked Report')
	, ( 5, 'Data Source')
	, ( 6, 'Report Model')
	, ( 8, 'Shared Dataset')
	, ( 9, 'Report Part')
	) tbl ([TypeID], [TypeDescription]) 
	WHERE TypeID = 1
)
SELECT 
	[ReportFolder] = c.Path --SUBSTRING(c.Path, 2, Len(c.Path)-Len(c.Name)-2)   
	, [OrderNumber] = 2
FROM 
	ReportServer.dbo.Catalog AS c WITH (NOLOCK) 
	INNER JOIN catalog_type_description AS ctd ON ctd.TypeID = c.[Type]
WHERE 
		1=1
		AND c.[Name] NOT IN('System Resources', 'UniversalBrand', 'fbac82c8-9bad-4dba-929f-c04e7ca4111f')
UNION 
SELECT @all_value, 1
ORDER BY 2, 1</CommandText>
      </Query>
      <Fields>
        <Field Name="ReportFolder">
          <DataField>ReportFolder</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="OrderNumber">
          <DataField>OrderNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PermissionKey">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <CommandText>SELECT tbl.* FROM (VALUES
  ( 'Browser', 'May view folders, reports and subscribe to reports.')
, ( 'Content Manager', 'May manage content in the Report Server. This includes folders, reports and resources.')
, ( 'Manage Subscriptions', 'Manage report subscriptions')
, ( 'My Reports', 'May publish reports and linked reports; manage folders, reports and resources in a userâ€™s My Reports folder.')
, ( 'NavigateFolder', 'Allow users to view and navigate child report folders')
, ( 'Publisher', 'May publish reports and linked reports to the Report Server.')
, ( 'Report Builder', 'May view report definitions.')
) tbl ([Role], [Description]) 
</CommandText>
      </Query>
      <Fields>
        <Field Name="Role">
          <DataField>Role</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
