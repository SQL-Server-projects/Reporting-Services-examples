  <DataSets>
    <DataSet Name="Variables">
      <Query>
        <DataSourceName>ReportServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ItemID">
            <Value>=Parameters!ItemID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH
XMLNAMESPACES 
( 
	DEFAULT 'http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition', 
    'http://schemas.microsoft.com/SQLServer/reporting/reportdesigner' AS rd 
)
, 
report_list
AS
(
	SELECT 
		  cat.ItemID
		, cat.[Name]
		, ReportXml = CAST(CAST(cat.Content AS VARBINARY(MAX)) AS XML)
	FROM  
		ReportServer.dbo.[Catalog] AS cat
	WHERE 
		1=1
		AND cat.Content IS NOT NULL
		AND cat.[Type] = 2 
		AND cat.ItemID IN(@ItemID)
)
SELECT  
	  rpt.ItemID
	, report_name = rpt.[name]
	, variable_name = x.value('@Name[1]', 'VARCHAR(50)') 
	, variable_value = x.value('Value[1]', 'VARCHAR(50)') 
FROM 
	report_list AS rpt
	CROSS APPLY ReportXml.nodes('/Report/Variables/Variable') r ( x )</CommandText>
      </Query>
