  <Variables>
    <Variable Name="ColorTableHeader">
      <Value>Gainsboro</Value>
    </Variable>
    <Variable Name="FormatDate">
      <Value>dd-MMM-yyyy</Value>
    </Variable>
    <Variable Name="FormatDateTime">
      <Value>dd-MMM-yyyy  hh:mm tt</Value>
    </Variable>
    <Variable Name="ReportExecutionTime">
      <Value>
        =  Right("00" + (System.DateTime.Now.Subtract(Globals!ExecutionTime).Hours).ToString, 2) + ":"
        + Right("00" + (System.DateTime.Now.Subtract(Globals!ExecutionTime).Minutes).ToString, 2) + ":"
        + Right("00" + (System.DateTime.Now.Subtract(Globals!ExecutionTime).Seconds).ToString, 2)
      </Value>
    </Variable>
    <Variable Name="ReportRunDateTime">
      <Value>=Format(DateTime.Now, Variables!FormatDateTime.Value)</Value>
    </Variable>
    <Variable Name="UrlReport">
      <Value>
        =Globals!ReportServerUrl + "/ReportServer?"
        + Replace(Globals!ReportFolder, " ", "+") + "%2f"
        + Replace(Globals!ReportName, " ", "+") + "&amp;rs:Command=Render"
      </Value>
    </Variable>
    <Variable Name="UrlReportWithParameters">
      <Value>=Variables!UrlReport.Value
 + "&amp;Server=" + Parameters!Server.Value
 + "&amp;Enabled=" + Parameters!Enabled.Value
 + "&amp;Category_Name=" + Join(Parameters!Category_Name.Value, "&amp;Category_Name=")
 + "&amp;Day_Of_Week=" + Parameters!Day_Of_Week.Value
 + "&amp;Is_Collasped=" + Parameters!Is_Collasped.Value
 + "&amp;Job_Status=" + Join(Parameters!Job_Status.Value, "&amp;Job_Status=")
 + IIf(IsNothing(Parameters!RunDate.Value), "&amp;RunDate:IsNull=True", "&amp;RunDate=" + Format(Parameters!RunDate.Value, Variables!FormatDate.Value))
 + IIf(IsNothing(Parameters!Job_Name.Value), "&amp;Job_Name:IsNull=True", "&amp;Job_Name=" + Parameters!Job_Name.Value)</Value>
    </Variable>
    <Variable Name="UserName">
      <Value>=Right(User!UserID, len(User!UserID)-instr(User!UserID, "\"))</Value>
    </Variable>
  </Variables>
