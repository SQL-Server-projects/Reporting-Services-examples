    <DataSet Name="Category">
      <Query>
        <DataSourceName>MSDB</DataSourceName>
        <CommandText>SELECT 
	category_name = c.name
FROM  
	msdb.dbo.sysjobs AS j 
	INNER JOIN msdb.dbo.syscategories as c on j.category_id = c.category_id
UNION SELECT '&lt;ALL&gt;'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="category_name">
          <DataField>category_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Jobs">
      <Query>
        <DataSourceName>MSDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Enabled">
            <Value>=Parameters!Enabled.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Category_Name">
            <Value>=Parameters!Category_Name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	job_name = j.name
FROM 
	msdb.dbo.sysjobs AS j 
	INNER JOIN msdb.dbo.sysjobschedules AS js ON j.job_id = js.job_id
	INNER JOIN msdb.dbo.syscategories as c on j.category_id = c.category_id
	INNER JOIN dbo.sysschedules s ON js.schedule_id = s.schedule_id
WHERE 
	1=1
	AND (CAST(j.enabled AS INT) = @Enabled OR 2 IN(@Enabled)) 
	AND (s.freq_relative_interval = 0) -- only weekly jobs not monthly or one off
	AND (s.freq_type NOT IN (1)) -- only weekly jobs not monthly or one off
	AND ('&lt;ALL&gt;' IN(@Category_Name) OR c.name IN(@Category_Name))
UNION SELECT '&lt;ALL&gt;'
ORDER BY 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="job_name">
          <DataField>job_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
