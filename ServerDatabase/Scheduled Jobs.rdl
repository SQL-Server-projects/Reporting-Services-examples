<?xml version="1.0" encoding="utf-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <Description>A list of scheduled jobs in SQL Server</Description>
  <Author>Anthony Duguid</Author>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="MSDB">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>="data source=" &amp; Parameters!Server.Value &amp; ";Initial Catalog=MSDB"</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>4610eb81-a43a-4aa2-b78c-cb5df771f6c8</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet">
      <Query>
        <DataSourceName>MSDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Enabled">
            <Value>=Parameters!Enabled.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Job_Name">
            <Value>=Parameters!Job_Name.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Category_Name">
            <Value>=Parameters!Category_Name.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RunDate">
            <Value>=Parameters!RunDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Day_Of_Week">
            <Value>=Parameters!Day_Of_Week.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Job_Status">
            <Value>=Parameters!Job_Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*'---------------------------------------------------------------------------------------
' Purpose:				Sql Server Job Schedule
' Notes:				Original sql from http://solihinho.wordpress.com/2009/01/01/query-for-listing-sql-server-job-schedule/
'						#CurrentJobStatus sql from http://www.sqlservercentral.com/Forums/Topic259078-8-1.aspx
'-----------------------------------------------------------------------------------------

DECLARE @Day_Of_Week AS varchar(1)
DECLARE @Job_Name AS varchar(200)
DECLARE @Category_Name AS varchar(100)
DECLARE @Enabled AS int
DECLARE @Job_Status AS INT
DECLARE @RunDate AS DATETIME

SET @Day_Of_Week = '0'
SET @Job_Name = NULL
SET @Category_Name = '&lt;ALL&gt;'
SET @Enabled = 2
SET @Job_Status = 4  -- 0='&lt;ALL&gt;'
SET @RunDate = NULL

*/

DECLARE @is_sysadmin INT
DECLARE @job_owner   sysname

IF OBJECT_ID('tempdb..#xp_results') IS NOT NULL
BEGIN
    DROP TABLE #xp_results
END

CREATE TABLE #xp_results (
     job_id                UNIQUEIDENTIFIER NOT NULL,
     last_run_date         INT              NOT NULL,
     last_run_time         INT              NOT NULL,
     next_run_date         INT              NOT NULL,
     next_run_time         INT              NOT NULL,
     next_run_schedule_id  INT              NOT NULL,
     requested_to_run      INT              NOT NULL,
     request_source        INT              NOT NULL,
     request_source_id     sysname          COLLATE database_default NULL,
     running               INT              NOT NULL,
     current_step          INT              NOT NULL,
     current_retry_attempt INT              NOT NULL,
     job_state             INT              NOT NULL)

SELECT @is_sysadmin = ISNULL(IS_SRVROLEMEMBER(N'sysadmin'), 0)
SELECT @job_owner = SUSER_SNAME()

INSERT INTO #xp_results
    EXECUTE master.dbo.xp_sqlagent_enum_jobs @is_sysadmin, @job_owner


SELECT
  job_name = j.Name
, category_name = c.Name
, job_enabled = 
	CASE j.enabled 
		WHEN 1 THEN 'Yes' 
		ELSE 'No' 
	END
, job_scheduled = 
	CASE s.enabled 
		WHEN 1 THEN 'Yes' 
		ELSE 'No' 
	END
, job_description = j.description
, schedule = 
	CASE s.freq_type 
		WHEN 1 THEN 'Once' 
		WHEN 4 THEN 'Daily' 
		WHEN 8 THEN 'Wk ' 
		+ CASE s.freq_interval &amp; 2 WHEN 2 THEN 'M' ELSE '' END 
		+ CASE s.freq_interval &amp; 4 WHEN 4 THEN 'Tu' ELSE '' END 
		+ CASE s.freq_interval &amp; 8 WHEN 8 THEN 'W' ELSE '' END 
		+ CASE s.freq_interval &amp; 16 WHEN 16 THEN 'Th' ELSE '' END
		+ CASE s.freq_interval &amp; 32 WHEN 32 THEN 'F' ELSE '' END 
		+ CASE s.freq_interval &amp; 64 WHEN 64 THEN 'Sa' ELSE '' END 
		+ CASE s.freq_interval &amp; 1 WHEN 1 THEN 'Su' ELSE '' END 
			WHEN 16 THEN 'Mthly on day ' + CONVERT(varchar(2), s.freq_interval) 
			WHEN 32 THEN 'Mthly ' + CASE s.freq_relative_interval 
			WHEN 1 THEN 'Every First ' 
			WHEN 2 THEN 'Every Second ' 
			WHEN 4 THEN 'Every Third ' 
			WHEN 8 THEN 'Every Fourth ' 
			WHEN 16 THEN 'Every Last ' END 
		+ CASE freq_interval 
			WHEN 1 THEN 'Sunday' 
			WHEN 2 THEN 'Monday' 
			WHEN 3 THEN 'Tuesday'
			WHEN 4 THEN 'Wednesday' 
			WHEN 5 THEN 'Thursday' 
			WHEN 6 THEN 'Friday' 
			WHEN 7 THEN 'Saturday' 
			WHEN 8 THEN 'Day' 
			WHEN 9 THEN 'Week day'
			WHEN 10 THEN 'Weekend day' END 
		WHEN 64 THEN 'Startup' 
		WHEN 128 THEN 'Idle' 
		ELSE 'Err' 
	END
, Occurs = 
	CASE s.freq_type
		WHEN  1 THEN 'Once'
		WHEN  4 THEN 'Daily'
		WHEN  8 THEN 'Weekly'
		WHEN 16 THEN 'Monthly'
		WHEN 32 THEN 'Monthly relative'
		WHEN 64 THEN 'When SQL Server Agent starts'
		WHEN 128 THEN 'Start whenever the CPU(s) become idle' 
	END
, schedule2 = 
	CASE s.freq_type
		WHEN  1 THEN 'O'
		WHEN  4 THEN 'Every '
			+ convert(varchar,s.freq_interval)
			+ ' day(s)'
		WHEN  8 THEN 'Every '
			+ convert(varchar,s.freq_recurrence_factor)
			+ ' weeks(s) on '
			+ CASE s.freq_interval 
				WHEN 1 THEN 'Sunday' 
				WHEN 2 THEN 'Monday' 
				WHEN 3 THEN 'Tuesday'
				WHEN 4 THEN 'Wednesday' 
				WHEN 5 THEN 'Thursday' 
				WHEN 6 THEN 'Friday' 
				WHEN 7 THEN 'Saturday' 
				WHEN 8 THEN 'Day' 
				WHEN 9 THEN 'Week day'
				WHEN 10 THEN 'Weekend day'
				WHEN 64 THEN 'Startup' 
				WHEN 128 THEN 'Idle' ELSE 'Err' END
		WHEN 16 THEN 'Day ' + convert(varchar,s.freq_interval)
			+ ' of every '
			+ convert(varchar,s.freq_recurrence_factor)
			+ ' month(s)'
		WHEN 32 THEN 'The '
			+ CASE s.freq_relative_interval
				WHEN  1 THEN 'First'
				WHEN  2 THEN 'Second'
				WHEN  4 THEN 'Third'
				WHEN  8 THEN 'Fourth'
				WHEN 16 THEN 'Last' END
			+ CASE s.freq_interval
				WHEN  1 THEN ' Sunday'
				WHEN  2 THEN ' Monday'
				WHEN  3 THEN ' Tuesday'
				WHEN  4 THEN ' Wednesday'
				WHEN  5 THEN ' Thursday'
				WHEN  6 THEN ' Friday'
				WHEN  7 THEN ' Saturday'
				WHEN  8 THEN ' Day'
				WHEN  9 THEN ' Weekday'
				WHEN 10 THEN ' Weekend Day' END
			+ ' of every '
			+ convert(varchar,s.freq_recurrence_factor)
			+ ' month(s)' 
	END
, j.enabled
, s.freq_type 
, s.freq_interval
, s.freq_relative_interval
, sch_Sun = 
	CASE 
		WHEN s.freq_type = 4 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 1 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 127 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 1 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 8 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 10 THEN 'Y' 
		ELSE 'N' 
	END
, sch_Mon = 
	CASE 
		WHEN s.freq_type = 4 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 2 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 127 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 2 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 8 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 9 THEN 'Y' 
		ELSE 'N' 
	END  
, sch_Tue = 
	CASE 
		WHEN s.freq_type = 4 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 4 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 127 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 3 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 8 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 9 THEN 'Y' 
		ELSE 'N' 
	END  
, sch_Wed = 
	CASE 
		WHEN s.freq_type = 4 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 8 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 127 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 4 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 8 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 9 THEN 'Y' 
		ELSE 'N' 
	END 
, sch_Thr = 
	CASE 
		WHEN s.freq_type = 4 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 16 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 127 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 5 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 8 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 9 THEN 'Y' 
		ELSE 'N' 
	END 
, sch_Fri = 
	CASE 
		WHEN s.freq_type = 4 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 32 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 127 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 6 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 8 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 9 THEN 'Y' 
		ELSE 'N' 
	END
, sch_Sat = 
	CASE 
		WHEN s.freq_type = 4 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 64 THEN 'Y' 
		WHEN s.freq_type = 8 AND s.freq_interval = 127 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 7 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 8 THEN 'Y' 
		WHEN s.freq_type = 32 AND s.freq_interval = 10 THEN 'Y' 
		ELSE 'N' 
	END
, DayOfWeek = 
	CASE 
		WHEN freq_type = 4 THEN '1' 
		WHEN freq_type = 8 AND freq_interval = 1 THEN '1' 
		WHEN freq_type = 8 AND freq_interval = 127 THEN '1' 
		WHEN freq_type = 32 AND freq_interval = 1 THEN '1' 
		WHEN freq_type = 32 AND freq_interval = 8 THEN '1' 
		WHEN freq_type = 32 AND freq_interval = 10 THEN '1' ELSE '' END 
		+ CASE 
			WHEN freq_type = 4 THEN '2' 
			WHEN freq_type = 8 AND freq_interval = 2 THEN '2' 
			WHEN freq_type = 8 AND freq_interval = 127 THEN '2' 
			WHEN freq_type = 32 AND freq_interval = 2 THEN '2' 
			WHEN freq_type = 32 AND freq_interval = 8 THEN '2' 
			WHEN freq_type = 32 AND freq_interval = 9 THEN '2' ELSE '' END 
		+ CASE 
			WHEN freq_type = 4 THEN '3' 
			WHEN freq_type = 8 AND freq_interval = 4 THEN '3' 
			WHEN freq_type = 8 AND freq_interval = 127 THEN '3' 
			WHEN freq_type = 32 AND freq_interval = 3 THEN '3' 
			WHEN freq_type = 32 AND freq_interval = 8 THEN '3' 
			WHEN freq_type = 32 AND freq_interval = 9 THEN '3' ELSE '' END 
		+ CASE 
			WHEN freq_type = 4 THEN '4' 
			WHEN freq_type = 8 AND freq_interval = 8 THEN '4' 
			WHEN freq_type = 8 AND freq_interval = 127 THEN '4' 
			WHEN freq_type = 32 AND freq_interval = 4 THEN '4' 
			WHEN freq_type = 32 AND freq_interval = 8 THEN '4' 
			WHEN freq_type = 32 AND freq_interval = 9 THEN '4' ELSE '' END 
		+ CASE 
			WHEN freq_type = 4 THEN '5' 
			WHEN freq_type = 8 AND freq_interval = 16 THEN '5' 
			WHEN freq_type = 8 AND freq_interval = 127 THEN '5' 
			WHEN freq_type = 32 AND freq_interval = 5 THEN '5' 
			WHEN freq_type = 32 AND freq_interval = 8 THEN '5' 
			WHEN freq_type = 32 AND freq_interval = 9 THEN '5' ELSE '' END 
		+ CASE 
			WHEN freq_type = 4 THEN '6' 
			WHEN freq_type = 8 AND freq_interval = 32 THEN '6' 
			WHEN freq_type = 8 AND freq_interval = 127 THEN '6' 
			WHEN freq_type = 32 AND freq_interval = 6 THEN '6' 
			WHEN freq_type = 32 AND freq_interval = 8 THEN '6' 
			WHEN freq_type = 32 AND freq_interval = 9 THEN '6' ELSE '' END 
		+ CASE 
			WHEN freq_type = 4 THEN '7' 
			WHEN freq_type = 8 AND freq_interval = 64 THEN '7' 
			WHEN freq_type = 8 AND freq_interval = 127 THEN '7' 
			WHEN freq_type = 32 AND freq_interval = 7 THEN '7' 
			WHEN freq_type = 32 AND freq_interval = 8 THEN '7' 
			WHEN freq_type = 32 AND freq_interval = 10 THEN '7' ELSE '' 
	END 
/*, CASE s.freq_subday_type
     WHEN 1 THEN 'Occurs once at '
        + master.dbo.fn_Time2Str(s.active_start_time)
     WHEN 2 THEN 'Occurs every '
        + convert(varchar,s.freq_subday_interval)
        + ' Seconds(s) Starting at '
        + master.dbo.fn_Time2Str(s.active_start_time)
        + ' ending at '
        + master.dbo.fn_Time2Str(s.active_end_time)
     WHEN 4 THEN 'Occurs every '
        + convert(varchar,s.freq_subday_interval)
        + ' Minute(s) Starting at '
        + master.dbo.fn_Time2Str(s.active_start_time)
        + ' ending at '
        + master.dbo.fn_Time2Str(s.active_end_time)
     WHEN 8 THEN 'Occurs every '
        + convert(varchar,s.freq_subday_interval)
        + ' Hour(s) Starting at '
        + master.dbo.fn_Time2Str(s.active_start_time)
        + ' ending at '
        + master.dbo.fn_Time2Str(s.active_end_time) END AS Frequency
*/
, frequency = 
	CASE s.freq_subday_type 
		WHEN 1 THEN 'Runs once' 
		WHEN 2 THEN 'every ' + CONVERT(varchar(3), freq_subday_interval) + ' seconds' 
		WHEN 4 THEN 'every ' + CONVERT(varchar(3), freq_subday_interval) + ' minutes' 
		WHEN 8 THEN 'every ' + CONVERT(varchar(3), freq_subday_interval) + ' hours' 
	END  
, LeftBuffer = DATEDIFF(minute, '00:00:00', SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_start_time), 6), 1, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_start_time), 6), 3, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_start_time), 6), 5, 2)) 
, StartTime = SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_start_time), 6), 1, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_start_time), 6), 3, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_start_time), 6), 5, 2)  
, RunTime = DATEDIFF(minute, SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_start_time), 6), 1, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_start_time), 6), 3, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_start_time), 6), 5, 2), SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_end_time), 6), 1, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_end_time), 6), 3, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_end_time), 6), 5, 2))  
, EndTime = SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_end_time), 6), 1, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_end_time), 6), 3, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_end_time), 6), 5, 2) 
, RightBuffer = DATEDIFF(minute, SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_end_time), 6), 1, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_end_time), 6), 3, 2) + ':' + SUBSTRING(RIGHT(STUFF(' ', 1, 1, '000000') + CONVERT(varchar(6), s.active_end_time), 6), 5, 2), '23:59:59')   
, h.*
, run_duration_format = LEFT(RIGHT('000000' + CAST(h.run_duration AS VARCHAR(10)),6),2) + ':' +
	SUBSTRING(RIGHT('000000' + CAST(h.run_duration AS VARCHAR(10)),6),3,2) + ':' +
	RIGHT(RIGHT('000000' + CAST(h.run_duration AS VARCHAR(10)),6),2) 
, run_time_format = CAST (CONVERT(VARCHAR, h.run_date) + ' ' + STUFF(STUFF(RIGHT('000000' + CONVERT(VARCHAR,h.run_time),6),5,0,':'),3,0,':') AS DATETIME) 
/*, CurrentStatus = 
	CASE 
		WHEN h.run_status IS NULL THEN 6
		WHEN cjs.current_execution_status IS NULL THEN h.run_priority
		WHEN cjs.current_execution_status IS NOT NULL THEN 5
		ELSE NULL 
	END  */
, CASE WHEN xp.job_state = 1 THEN 5
		WHEN h.run_status IS NULL THEN 6
		ELSE h.run_priority END AS CurrentStatus
/*
, Duration = 
	CASE 
		WHEN s.freq_type =  1 THEN 'On date: '
          + master.dbo.fn_Date2Str(s.active_start_date)
          + ' At time: '
          + master.dbo.fn_Time2Str(s.active_start_time)
		WHEN s.freq_type &lt; 64 THEN 'Start date: '
          + master.dbo.fn_Date2Str(s.active_start_date)
          + ' end date: '
          + master.dbo.fn_Date2Str(s.active_end_date) 
	END
, Next_Run_Date = master.dbo.fn_Date2Str(xp.next_run_date) + ' ' + master.dbo.fn_Time2Str(xp.next_run_time)
*/
FROM  
	dbo.sysjobs j 
	INNER JOIN dbo.sysjobschedules js ON j.job_id = js.job_id
	INNER JOIN dbo.sysschedules s ON js.schedule_id = s.schedule_id
	INNER JOIN dbo.syscategories c ON j.category_id = c.category_id
	LEFT JOIN (SELECT  
					sh.*
					, run_status_desc = 
						CASE CAST(sh.run_status AS NVARCHAR(11))
							WHEN 0 THEN 'Failed'
							WHEN 1 THEN 'Succeeded'
							WHEN 2 THEN 'Retry'
							WHEN 3 THEN 'Canceled'
							WHEN 4 THEN 'In Progress'
						END 
					, run_priority = 
						CASE CAST(sh.run_status AS NVARCHAR(11))
						WHEN 0 THEN 4
						WHEN 1 THEN 1
						WHEN 2 THEN 3
						WHEN 3 THEN 2 
						WHEN 4 THEN 5
						END
				FROM 
					MSDB.dbo.sysjobhistory sh (NOLOCK)) h ON j.job_id = h.job_id
	INNER JOIN #xp_results xp ON j.job_id = xp.job_id
WHERE 
	1=1	
	AND (CAST(j.enabled AS INT) = @Enabled OR 2 IN(@Enabled)) 
	AND (s.freq_relative_interval = 0) -- only weekly jobs not monthly or one off
	AND (s.freq_type NOT IN (1)) -- only weekly jobs not monthly or one off
	AND (@Job_Name IS NULL OR j.name LIKE '%' + @Job_Name + '%')
	AND ('&lt;ALL&gt;' IN(@Category_Name) OR c.name IN(@Category_Name))
	AND ((CAST (CONVERT(VARCHAR, h.run_date) + ' ' + STUFF(STUFF(RIGHT('000000' + CONVERT(VARCHAR,h.run_time),6),5,0,':'),3,0,':') AS DATETIME)) &gt;= @RunDate OR @RunDate IS NULL)
	--AND (CHARINDEX(@Day_Of_Week, a.DayOfWeek) &gt; '0' OR @Day_Of_Week = '0')
	AND (
		CASE 
			WHEN xp.job_state = 1 THEN 5
			WHEN h.run_status IS NULL THEN 6
			ELSE h.run_priority 
		END IN(@Job_Status) OR 0 IN (@Job_Status))
ORDER BY j.name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="job_name">
          <DataField>job_name</DataField>
        </Field>
        <Field Name="job_description">
          <DataField>job_description</DataField>
        </Field>
        <Field Name="job_enabled">
          <DataField>job_enabled</DataField>
        </Field>
        <Field Name="category_name">
          <DataField>category_name</DataField>
        </Field>
        <Field Name="schedule">
          <DataField>schedule</DataField>
        </Field>
        <Field Name="enabled">
          <DataField>enabled</DataField>
        </Field>
        <Field Name="freq_type">
          <DataField>freq_type</DataField>
        </Field>
        <Field Name="freq_interval">
          <DataField>freq_interval</DataField>
        </Field>
        <Field Name="freq_relative_interval">
          <DataField>freq_relative_interval</DataField>
        </Field>
        <Field Name="sch_Sun">
          <DataField>sch_Sun</DataField>
        </Field>
        <Field Name="sch_Mon">
          <DataField>sch_Mon</DataField>
        </Field>
        <Field Name="sch_Tue">
          <DataField>sch_Tue</DataField>
        </Field>
        <Field Name="sch_Wed">
          <DataField>sch_Wed</DataField>
        </Field>
        <Field Name="sch_Thr">
          <DataField>sch_Thr</DataField>
        </Field>
        <Field Name="sch_Fri">
          <DataField>sch_Fri</DataField>
        </Field>
        <Field Name="sch_Sat">
          <DataField>sch_Sat</DataField>
        </Field>
        <Field Name="frequency">
          <DataField>frequency</DataField>
        </Field>
        <Field Name="LeftBuffer">
          <DataField>LeftBuffer</DataField>
        </Field>
        <Field Name="StartTime">
          <DataField>StartTime</DataField>
        </Field>
        <Field Name="RunTime">
          <DataField>RunTime</DataField>
        </Field>
        <Field Name="EndTime">
          <DataField>EndTime</DataField>
        </Field>
        <Field Name="RightBuffer">
          <DataField>RightBuffer</DataField>
        </Field>
        <Field Name="instance_id">
          <DataField>instance_id</DataField>
        </Field>
        <Field Name="job_id">
          <DataField>job_id</DataField>
        </Field>
        <Field Name="step_id">
          <DataField>step_id</DataField>
        </Field>
        <Field Name="step_name">
          <DataField>step_name</DataField>
        </Field>
        <Field Name="sql_message_id">
          <DataField>sql_message_id</DataField>
        </Field>
