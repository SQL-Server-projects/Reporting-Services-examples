<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <Description>A list of scheduled jobs in SQL Server</Description>
  <Author>Anthony Duguid</Author>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DynamicSource">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>="data source=" &amp; Parameters!Server.Value &amp; ";initial catalog=msdb"</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>856aa19d-7b0a-4542-894d-3038cca76358</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Jobs">
      <Query>
        <DataSourceName>DynamicSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CommandSearch">
            <Value>=Parameters!CommandSearch.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Category">
            <Value>=Parameters!Category.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	  ServerName = @@ServerName
	, jb.name
	, jb.[DESCRIPTION]
	, jb.Category_id
	, cg.Category_Class
	, cg.Category_type
	, [Category] = cg.[name]
	, js.database_name 
	, js.step_id 
	, js.step_name 
	, js.command 
FROM 
	dbo.sysjobs as jb with (NOLOCK)
	INNER JOIN dbo.sysjobsteps AS js WITH (NOLOCK) ON jb.job_id = js.job_id 
	LEFT OUTER JOIN dbo.syscategories as cg with (NOLOCK) on jb.category_id = cg.Category_ID
WHERE  
	1=1
	AND (('&lt;ALL&gt;' IN (@Category))  OR ( cg.[name]  IN(@Category)))
	AND 
		(
		@CommandSearch IS NULL 
		OR (jb.name LIKE '%' + @CommandSearch + '%') 
		OR (js.command LIKE '%' + @CommandSearch + '%') 
		OR (js.step_name LIKE '%' + @CommandSearch + '%')
		)
ORDER BY 
	  jb.name
	, js.step_id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ServerName">
          <DataField>ServerName</DataField>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
        </Field>
        <Field Name="DESCRIPTION">
          <DataField>Description</DataField>
        </Field>
        <Field Name="category_id">
          <DataField>category_id</DataField>
        </Field>
        <Field Name="category_class">
          <DataField>category_class</DataField>
        </Field>
        <Field Name="category_type">
          <DataField>category_type</DataField>
        </Field>
        <Field Name="Category">
          <DataField>Category</DataField>
        </Field>
        <Field Name="database_name">
          <DataField>database_name</DataField>
        </Field>
        <Field Name="step_id">
          <DataField>step_id</DataField>
        </Field>
        <Field Name="step_name">
          <DataField>step_name</DataField>
        </Field>
        <Field Name="command">
          <DataField>command</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Categories">
      <Query>
        <DataSourceName>DynamicSource</DataSourceName>
        <CommandText>;WITH
categories
AS
(
	SELECT 
		  SortORder = CAST(1 AS INTEGER)
		, name 
	FROM 
		dbo.syscategories WITH (NOLOCK)
	UNION ALL 
	SELECT 
		  0
		, '&lt;ALL&gt;'
)
SELECT 
	C.Name 
FROM
	categories AS C
ORDER BY 
	  C.SortOrder
	, C.Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
        </Field>
        <Field Name="SortOrder">
          <DataField>SortOrder</DataField>
        </Field>
      </Fields>
